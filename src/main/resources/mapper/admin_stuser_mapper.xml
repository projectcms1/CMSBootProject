<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cms.ca.admin.stuser_repo">

<!--학생리스트 출력-->
<select id="student_list" resultType="com.cms.ca.student_dto" >
select * from STDNT_INFO order by STDNT_NO desc
</select>

<!--학생 정보 검색 (학번 기준)-->
<select id="findby_studentnumber" resultType="com.cms.ca.student_dto">
select * from STDNT_INFO where STDNT_NO=#{STDNT_NO};
</select>

</mapper>